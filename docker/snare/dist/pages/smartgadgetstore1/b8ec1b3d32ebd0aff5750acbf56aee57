    background-color: #262626; /* Dark background */
    border: none; /* Remove borders */
    box-shadow: none; /* Remove shadow */
    width: 100%; /* Match sidenav width */
}

.categories-dropdown .dropdown-item {
    color: #ffffff; /* White text */
    font-size: 20px; /* Match font size */
    padding: 10px 30px; /* Adjust padding */
    text-align: left; /* Align text to the left */
}

.categories-dropdown .dropdown-item:hover {
    background-color: #444; /* Darken on hover */
    color: #f1f1f1; /* Lighter text */
}

/* Remove focus outline (blue border) */
.categories-dropdown .dropdown-toggle:focus {
    outline: none;  /* Removes the focus outline */
    box-shadow: none; /* Removes the box-shadow */
}


