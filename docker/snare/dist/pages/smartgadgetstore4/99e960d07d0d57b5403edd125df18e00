      <script>
         function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
         }

         function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
         }

         // Restore dropdown and popup functionality
         $(document).ready(function() {
            // Enable dropdowns
            $('.dropdown-toggle').dropdown();

            // Popup trigger
            $(".trigger-popup").on("click", function(e){
               e.preventDefault();
               $("#loginPopup").fadeIn();
            });

            $(".close-btn").on("click", function(){
               $("#loginPopup").fadeOut();
            });
         });
      </script>

        <script>
            $(document).ready(function () {
            const productGrid = document.querySelector('.product_grid');
            const products = Array.from(productGrid.children);
        
            for (let i = products.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                productGrid.appendChild(products[j]); // Move product to the end
                products.splice(j, 1); // Remove the moved product from array
            }
            });
        </script>
 

